{
  "remote": {
    "remoteEntry": "http://localhost:4201/remoteEntry.js",
    "frame": "identical",
    "pages": [
      {
        "exposed": "./Module",
        "displayName": "远端程序",
        "routePath": "remote",
        "moduleName": "EntryModule"
      }
    ],
    "components": [
      {
        "exposed": "./board",
        "moduleName": "BoardComponent"
      }
    ]
  },
  "remote-vue": {
    "remoteEntry": "http://localhost:8080/assets/remoteEntry.js",
    "frame": "outside",
    "pages": [
      {
        "exposed": "./RemoteVue",
        "displayName": "VUE",
        "routePath": "vue",
        "elementName": "vue-element"
      }
    ]
  },
  "remote-react": {
    "remoteEntry": "http://localhost:3000/remoteEntry.js",
    "frame": "outside",
    "pages": [
      {
        "exposed": "./Module",
        "displayName": "REACT",
        "routePath": "react",
        "elementName": "remote-react"
      }
    ],
    "components": [
      {
        "exposed": "./Components",
        "elementName": "react-board",
        "moduleName": "RemoteReactBoard"
      }
    ]
  }
}
